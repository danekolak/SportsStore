@model ProductsListViewModel
@foreach (var p in Model.Products)
{
    @Html.Partial("ProductSummary", p)
}
<div page-model="@Model.PagingInfo" page-action="List" page-classes-enabled="true" page-class-normal="btn btn-default"
     page-class-selected="btn btn-primary" page-url-category="@Model.CurrentCategory" class="btn btn-group pull-right">
</div>
